syntax = "v1"

info(
    title: "go-zero最佳实践"
    desc: "go-zero最佳实践"
    author: "zjzjzjzj"
    email: "zjzjzjzj1874@gmail.com"
    version: "v0"
)

type (
    LivenessResponse struct {
	    Msg string `json:"msg" description:"msg"`
    }
)

@server(
	prefix: task/
	group: task
)

service task {
    @doc(
        summary: "健康检查"
    )
    @handler liveness
    get /liveness  returns (LivenessResponse)
}